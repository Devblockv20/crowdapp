<%	provide(:title,	"| Profile Setting")%> 

<div class="container">
	<div class="row">
		<div class="col s6 offset-s3">
			<div class="card ">
				<div class="card-content black-text">
					<span class="card-title black-text ">Profile Setting</span>

					<p>Profile Setting</p>

					<%=	form_for(@user_profile)	do	|f|	%>
					<%= render(:partial => "form", :locals => {:f => f}) %>

					<%=	f.button "Update Setting", class: "btn waves-effect waves-light"%>
					<%= button_tag t('buttons.cancel'), type: "submit", name: "cancel", value: true %>
					<% end %>

				</div>
			</div>
		</div>
	</div>
</div>